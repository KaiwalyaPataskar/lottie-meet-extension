{
  "name": "Meet Animator",
  "description": "Replace humdrum profile with Lottie Animation.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "css": [],
      "run_at": "document_start",
      "js": ["lottie.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ]
}
